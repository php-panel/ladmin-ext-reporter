{
    "name": "php-panel/ladmin-ext-reporter",
    "description": "Exception reporter for laravel",
    "type": "library",
    "keywords": [
        "laravel-admin",
        "exception",
        "reporter"
    ],
    "homepage": "https://github.com/php-panel/ladmin-ext-reporter",
    "license": "MIT",
    "authors": [
        {
            "name": "leeqvip",
            "email": "leeqvip@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravel/framework": ">=12.0",
        "php-panel/laravel-admin": ">=3.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~11.0",
        "laravel/laravel": ">=12.0"
    },
    "autoload": {
        "psr-4": {
            "Ladmin\\Reporter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ladmin\\Reporter\\ReporterServiceProvider"
            ]
        }
    }
}